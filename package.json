  "scripts": {
    "react-dev": "webpack --watch --display-error-details",
    "start": "nodemon --inspect ./server/index.js",
    "test": "jest --coverage",
    "build": "webpack",
    "seed": "sudo node --trace_gc --inspect database/seed.js",
