    "babel-loader": "^8.0.2",
    "body-parser": "^1.18.3",
    "cassandra-driver": "^3.5.0",
    "cli": "^1.0.1",
    "cors": "^2.8.4",
    "css-loader": "^1.0.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.6.0",
